{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_NO_INSTALL": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_NO_INSTALL": "true"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_NO_INSTALL": "true"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
